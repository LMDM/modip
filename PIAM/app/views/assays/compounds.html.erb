<%= stylesheet_link_tag  'assays', :media => 'all' %>

<div class="row-fluid">
  <div class="row-fluid">
    <div class="span4">
      <%= page_entries_info @compounds %>
    </div>
    <div class="span6" style="text-align: right; margin-top: -20px;">
      <%= paginate @compounds %>
    </div>
  </div>
  <br />
  <% @compounds.each do |compound| %>
      <div class="compound-container row-fluid">
     <label>Name:</label>
     <strong> <%= compound.name %> </strong>
      <div class="rprtid">
        <label>CID:</label>
        <%= link_to compound.cid, compound_path(compound) %>
      </div>
      </div>
<% end %>
</div>

<div class="row-fluid" style="text-align: center;"> <%= paginate @compounds %> </div>