<table class="definition table table-bordered table-condensed">
		<thead>
			<tr>
				<th>Receptor Type</th>
				<th>Center X</th>
				<th>Center Y</th>
				<th>Center Z</th>
				<th>Size X</th>
				<th>Size Y</th>
				<th>Size Z</th>
				<th>Model Number</th>
				<th>Energy Range</th>
				<th>Use of CPU</th>
			</tr>
		</thead>
		<tbody>
			<% target.prepare_receptors.each do |receptor| %>
				<tr>
					<td><p class="text-info"><%= receptor.receptor_type.name %></p></td>
					<td><%= receptor.center_x %></td>
					<td><%= receptor.center_y %></td>
					<td><%= receptor.center_z %></td>
					<td><%= receptor.size_x %></td>
					<td><%= receptor.size_y %></td>
					<td><%= receptor.size_z %></td>
					<td><%= receptor.number_of_conformer ? receptor.number_of_conformer : "default = #{9}" %></td>
					<td><%= receptor.energy_range ? receptor.energy_range : "default = #{3}" %></td>
					<td><%= receptor.cpu_number ? receptor.cpu_number : "All cpu's available"%></td>
					<% if action %>
						<td> <%= link_to 'Show', target_prepare_receptor_path(@target, receptor) %> </td>
						<td> <%= link_to 'Edit', edit_target_prepare_receptor_path(@target, receptor) %> </td>
						<td> <%= delete_dialog(receptor.id, target_prepare_receptor_path(@target, receptor))%>  </td>
					<% end %>
				</tr>
			<% end %>
		</tbody>
	</table>
